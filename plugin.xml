<plugin name='cdn-resources' version='0.2.1' grailsVersion='2.1.1 &gt; *'>
  <author>Tomas Lin</author>
  <authorEmail>tomaslin@gmail.com</authorEmail>
  <title>Cdn Resources Plugin</title>
  <description>\
Loads static resources using Content Delivery Networks using the Resources plugin framework
</description>
  <documentation>https://github.com/tomaslin/grails-cdn-resources</documentation>
  <type>CdnResourcesGrailsPlugin</type>
  <resources>
    <resource>grails.plugin.cdnresources.CdnResourceMapper</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://download.newrelic.com/' url='http://download.newrelic.com/' />
    <repository name='http://grails.org/plugins' url='http://grails.org/plugins' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='org.quartz-scheduler' name='quartz' version='2.1.5' />
    </compile>
    <runtime>
      <dependency group='com.googlecode.json-simple' name='json-simple' version='1.1' />
      <dependency group='commons-codec' name='commons-codec' version='1.6' />
      <dependency group='mysql' name='mysql-connector-java' version='5.1.21' />
      <dependency group='joda-time' name='joda-time' version='2.1' />
    </runtime>
  </dependencies>
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='grails-melody' version='1.14' />
      <plugin group='org.grails.plugins' name='vero' version='0.1.0' />
      <plugin group='org.grails.plugins' name='kissmetrics' version='0.1.2' />
      <plugin group='org.grails.plugins' name='facebook-sdk' version='0.4.4' />
      <plugin group='org.grails.plugins' name='cache-ehcache' version='1.0.0' />
      <plugin group='org.grails.plugins' name='console' version='1.2' />
      <plugin group='org.grails.plugins' name='aws-sdk' version='1.3.24' />
      <plugin group='org.grails.plugins' name='cookie-session' version='2.0.1' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='resources' version='1.2.RC2' />
      <plugin group='org.grails.plugins' name='jquery' version='1.8.3' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.1.1' />
      <plugin group='org.grails.plugins' name='quartz2' version='0.2.3' />
    </runtime>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>